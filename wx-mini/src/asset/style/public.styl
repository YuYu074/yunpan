
@import '~@/asset/style/common.styl'
.van-checkbox__icon--square
  border-color theme !important
.DMSstore,.DSRstore
  font-size font-xs
  color text-lightest
  border-radius 50%
  width 18px
  height 18px
  flex 0 0 18px
  text-align center
  line-height 18px 
.DMSstore
  background-color theme
.DSRstore
  background-color #F99D14
.tags-wrap
  display flex
  padding 0 dis-s dis-xs
  .v-tag
    font-size font-xs
    margin-right dis-s
.divider
    height 8px
    width 100%
    background-color #F2F4F8
.required
  color #F32B1D
.time-wrapper
  flex 1
  display flex
  justify-content space-around
  border-radius radius-m
  background-color #F4F6F8
  padding 10px 0
  align-items center
  .left
    padding 0 dis-m
  .right
    padding 0 dis-m
  .middle
    flex 1
    display flex
    font-size font-m
    .toto
      color #ccc
    .start-time
      flex 1
      text-align center
    .end-time
      flex 1
      text-align center
.custom-class
  color text !important
  border-radius radius-m !important
.bt-custom-class
  border-radius radius-m !important
.field-class
  padding: 3px 10px 3px 0 !important
.custom-class
  padding 0 !important
.van-search__content
  background-color #F4F6F8 !important
  border-radius radius-m !important
.icon_search-wrap
  padding-right dis-s
.select-content
  display flex
  align-items center
  justify-content space-between
  background-color #F4F6F8
  height: 42px
  border-radius radius-m
  .xticon
    padding: 0 8px 0 5px
    height: 100%
    display: flex
    align-items: center
  .input
    flex 1
    padding-left: 8px
    width 100%
    height 100%
    font-size font-m
.XpaddingB
  padding-bottom: env(safe-area-inset-bottom) !important
.footer-wrap
  position fixed
  bottom 0
  background #fff
  z-index 9
  display flex
  width 100%
  padding dis-m 0
  z-index 2
  box-shadow 0px -3px 6px 1px rgba(0, 48, 130, 0.05)
  &>view 
    flex 1
    &.left
      padding 0 6px 0 12px
    &.right
      padding 0 12px 0 6px
